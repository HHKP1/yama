{
  "name": "ukr-yama",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "materialize-css": "^1.0.0-rc.2",
    "v-mask": "^2.2.3",
    "vue": "^2.6.11",
    "vue-moment": "^4.0.0",
    "vue-mq": "^1.0.1",
    "vue-router": "^3.4.9",
    "vue-toasted": "^1.1.28",
    "vuejs-datepicker": "^1.6.2",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/standard"
    ],
    "rules": {
      "no-console": "off",
      "indent": [
        "error",
        "tab"
      ],
      "no-tabs": "off",
      "semi": "off",
      "no-template-curly-in-string": "off",
      "quotes": "off",
      "keyword-spacing": "off",
      "space-before-blocks": "off",
      "curly": "off",
      "space-before-function-paren": "off",
      "comma-dangle": "off",
      "spaced-comment": "off",
      "space-infix-ops": "off",
      "eqeqeq": "off",
      "no-extend-native": "off",
      "eol-last": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
